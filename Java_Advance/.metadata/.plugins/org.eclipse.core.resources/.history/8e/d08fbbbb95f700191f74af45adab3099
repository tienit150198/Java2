package pro03;

public class Item {
	String name;
	double price;

	public Item() {
		super();
	}

	public Item(String name, double price) {
		super();
		this.name = name;
		this.price = price;
	}

	String getName() {
		return name;
	}

	double getPrice() {
		return price;
	}

	void setName(String name) {
		this.name = name;
	}

	void setPrice(double price) {
		this.price = price;
	}

}
