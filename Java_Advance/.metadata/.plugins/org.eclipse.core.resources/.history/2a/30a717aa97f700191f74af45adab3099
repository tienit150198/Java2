package pro03;

public class Book extends ShopItem {
	double weight;

	public Book() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Book(String id, String iName, String iPrice, int iQttt) {
		super(id, iName, iPrice, iQttt);
		// TODO Auto-generated constructor stub
	}

	public Book(double weight) {
		super();
		this.weight = weight;
	}
	
	

}
